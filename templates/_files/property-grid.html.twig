<div class="property-item">
    {# {{ property.tag }} #}
    <div class="img relative">
        {% if vich_uploader_asset(property, 'imageFile') is null %}
            <img class="w-full" src="{{ 'images/properties/empty.jpg'}}" alt="property">
        {% else %}
            <img class="w-full" src="{{ vich_uploader_asset(property, 'imageFile') }}" alt="property">
        {% endif %}
        <div class="read-more w-full bg-[#1c5588c4] p-[10px]">
            <p class="text-center font-medium text-white"><a href="{{ path('property.show', {id:property.id, slug:property.slug}) }}">Read More</a></p>
        </div>
        {% if property.tag  %}
            <div class="ribbon pt-serif">
                {{ property.tag }}
            </div>
        {% endif %}
    </div>
    <div class="property-infos px-[20px] pb-4">
        <div class="infos text-lg text-center">
            <h2 class="font-medium capitalize mt-2 text-[22px] ">
                <a class="block w-full hover:text-main" href="{{ path('property.show', {id:property.id, slug:property.slug}) }}">
                    <strong class="pt-serif">{{ property.truncatedString(property.title, 15)  }}</strong>
                </a>
            </h2>
            <p class="text-main font-medium text-[20px] my-[7px]">{{ property.formatedPrice }} $</p>
            <p class="text-back text-sm">{{ property.truncatedString(property.description, 35) }}</p>
        </div>
        <div class="details flex justify-center gap-[9px] flex-wrap mt-[20px]">
